            jQuery('#view_demos_mobile').bind('click', function (e) {
                e.preventDefault();
                e.stopPropagation();

                if (jQuery('.product-menu').is(':visible')) {
                    jQuery('.b-topnav').trigger('click');
                } else {
                    jQuery('#view-demo').trigger('click');
                }
            });
            jQuery('#view-port-desktop-user').bind('click', function (e) {
                e.preventDefault();
                jQuery('[data-viewport=desktop][data-type=user]').trigger('click');
            });
            jQuery('#view-port-tablet-user').bind('click', function (e) {
                e.preventDefault();
