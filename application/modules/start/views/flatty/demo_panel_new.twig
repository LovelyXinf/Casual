                jQuery('[data-viewport=mobile-h][data-type=user]').trigger('click');
            });
            jQuery('#view-port-mobile-user').bind('click', function (e) {
                e.preventDefault();
                jQuery('[data-viewport=mobile][data-type=user]').trigger('click');
            });
            jQuery('#view-port-desktop-admin').bind('click', function (e) {
                e.preventDefault();
                jQuery('[data-viewport=desktop][data-type=admin]').trigger('click');
            });
            jQuery('#view-port-tablet-admin').bind('click', function (e) {
                e.preventDefault();
                jQuery('[data-viewport=tablet][data-type=admin]').trigger('click');
            });
            jQuery('#btn-collapse').bind('click', function (e) {
                e.preventDefault();
                jQuery('#demo-panel').hide();
                if ($('.app-top-banner-js').is(':visible')) {
                    $('#frame-wrapper').css('height', 'calc(100vh - 97px)');
                }
                jQuery('#frame-wrapper').addClass('hide-panel');
                jQuery('#btn-collapse').addClass('hidden');
                jQuery('#btn-expand').removeClass('hidden');
                if (jQuery('.product-menu').is(':visible')) {
                    jQuery('.product-menu').hide();
                }

            });
            jQuery('#btn-expand').bind('click', function (e) {
                if ($('.app-top-banner-js').is(':visible')) {
                    $('#frame-wrapper').css('height', 'calc(100vh - 194px)');
                }
                e.preventDefault();
                jQuery('#demo-panel').show();
                jQuery('#frame-wrapper').removeClass('hide-panel');
                jQuery('#btn-collapse').removeClass('hidden');
                jQuery('#btn-expand').addClass('hidden');
                if (jQuery('.sandwich-button').hasClass('active')) {
                    jQuery('.product-menu').show();
                }
            });
            jQuery(function ($) {
                $('#demo_viewer_user_mode').bind('click', function (e) {
                    e.preventDefault();
                    localStorage.setItem('access_page', 'user');
                    $('#demo_viewer').prop('src', site_url + 'admin');
                    $('#demo_viewer_user_mode').hide();
                    $('#demo_viewer_admin_mode').show();
                    $('#view-port-mobile-h-user').hide();
                    $('#view-port-mobile-user').hide();
                    $('#view-port-desktop-user').hide();
                    $('#view-port-tablet-user').hide();
                    $('#view-port-desktop-admin').show();
                    $('#view-port-tablet-admin').show();
                    $('.get_demo_user-demo').hide();
                    $('.get_demo_admin-demo').show();

                    if ($('#view-port-tablet-user').hasClass('active')) {
                        $('#view-port-tablet-admin').trigger('click');
                    } else {
                        $('#view-port-desktop-admin').trigger('click');
                    }
                });
                $('#demo_viewer_admin_mode').bind('click', function (e) {
                    e.preventDefault();
                    localStorage.setItem('access_page', 'admin');
                    $('#demo_viewer').prop('src', site_url + 'start/index/1/1');
                    $('#demo_viewer_admin_mode').hide();
                    $('#demo_viewer_user_mode').show();
                    $('#view-port-mobile-h-user').show();
                    $('#view-port-mobile-user').show();
                    $('#view-port-desktop-user').show();
                    $('#view-port-tablet-user').show();
                    $('#view-port-desktop-admin').hide();
                    $('#view-port-tablet-admin').hide();
                    $('.get_demo_admin-demo').hide();
                    $('.get_demo_user-demo').show();

                    if ($('#view-port-tablet-admin').hasClass('active')) {
                        $('#view-port-tablet-user').trigger('click');
                    } else {
                        $('#view-port-desktop-user').trigger('click');
                    }
                });

                if (!isFramed) {
                    $('#demo-panel').show();
                    $('html').css('overflow', 'hidden');
                    var access_page = localStorage.getItem('access_page');
                    var cur_page = '{{ page }}';
                    if (cur_page == 'preview') {
                        $('#demo_viewer').prop('src', site_url + 'start/index/preview/1');
                    } else {
                        if (access_page) {
                            $('#demo_viewer_' + access_page + '_mode').trigger('click');
                        } else {
                            $('#demo_viewer').prop('src', site_url + 'start/index/1/1');
                        }
                    }
                } else {
                    $('html').css('overflow', 'visible');
                    $('#demo-panel').after($('.app-top-banner-js'));
                }
            });
        </script>
