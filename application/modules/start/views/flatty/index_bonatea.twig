{% if not is_pjax %}
    <!DOCTYPE html>
    <html DIR="{{ _LANG.rtl }}" lang="{{ _LANG.code }}" xmlns="https://www.w3.org/1999/xhtml" />
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
            <meta name="viewport" content="width=device-width, initial-scale=1">
                <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
                    {% helper seo.seo_tags('robots') %}

                    <base href="{{ site_root }}" />

                    {% helper start.favicon({'type': 'user'}) %}

                                    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700&amp;subset=cyrillic" rel="stylesheet"> 
                                        <link rel="stylesheet" href="{{ site_root }}uploads/themes-fixed/bonatea/styles/bootstrap-{{ _LANG.rtl }}.css" type="text/css" />
                                        <link rel="stylesheet" href="{{ site_root }}uploads/themes-fixed/bonatea/styles/font-awesome.css" type="text/css" />
                                        <link rel="stylesheet" href="{{ site_root }}application/views/flatty/css/fontawesome-5.0.11/css/fontawesome-all.min.css" type="text/css" /> 
                                        {% helper theme.include_css('style', 'screen') %}
                                        {% helper theme.css(load_type) %}
                                        <link rel="stylesheet" href="{{ site_root }}uploads/themes-fixed/bonatea/styles/style-{{ _LANG.rtl }}.css" type="text/css" />
                                        <style>
                                            @media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) { 
                                                body.index-bonatea  {
                                                    /* fix ie links margin on hover */
                                                    font-family: Arial, sans-serif;
                                                }
                                                .index-bonatea .list-inline li {

                                                    padding-right: 20px;
                                                    float: left;
                                                    height: 30px;

                                                }
                                                .index-bonatea .list-inline li:last-child {

                                                    padding-top: 5px;

                                                }
                                                .index-bonatea .navbar-right li{
                                                    padding-top: 5px;

                                                }

                                            }

                                        </style>

                                    {% endif %}

                                    {% helper seo.seo_tags('title') %}
                                    {% helper seo.seo_tags('description|keyword|canonical|og_title|og_type|og_url|og_image|og_site_name|og_description') %}

                                    <script>
                                        var site_rtl_settings = '{{ _LANG.rtl }}';
                                        var is_pjax = parseInt('{{ is_pjax }}');
                                        var js_events
                                        = {% helper json.json_encode(js_events) %};
                                                var id_user = {% if user_session_data.user_id %}{{ user_session_data.user_id }}{% else %}0{% endif %};
                                    </script>
                                    {% if not is_pjax %}
                                        <script>
                                            var site_url = '{{ site_url }}';
                                            var base_url = '{{ base_url }}';
                                            var img_folder = '{{ img_folder }}';
                                            var site_error_position = 'center';
                                            var use_pjax = 0;
                                            var pjax_container = '#pjaxcontainer';
                                        </script>
                                        {% helper utils.jscript('', 'jquery.js', '', 'sync') %}
                                        <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
                                        {% helper utils.jscript('', 'jquery.pjax.js', '', 'sync') %}
                                        {% helper utils.jscript('', 'functions.js') %}
                                        {% helper utils.jscript('', 'nprogress/nprogress.min.js') %}
                                        {% helper utils.jscript('', 'errors.js') %}
                                        {% helper utils.jscript('', 'loading_content.js') %}
                                        {% helper utils.jscript('', 'pginfo.js') %}
                                        {% helper utils.jscript('', 'alerts.js') %}
                                        {% helper utils.jscript('', 'notifications.js') %}
                                        {% helper seo_advanced.seo_traker('top') %}
                                        {% helper start.analytics() %}
                                        </head>
                                        <body class="index-bonatea">
                                            {% helper start.demo_panel({type: 'user', place: 'top'}) %}
                                            <div id="pjaxcontainer" class="hp100">
                                            {% endif %}
                                            <div id="error_block" class="hide">{% for item in _PREDEFINED.error %}{% if item.text %}{{ item.text }}<br>{% endif %}{% endfor %}</div>
                                            <div id="info_block" class="hide">{% for item in _PREDEFINED.info %}{% if item.text %}{{ item.text }}<br>{% endif %}{% endfor %}</div>
                                            <div id="success_block" class="hide">{% for item in _PREDEFINED.success %}{% if item.text %}{{ item.text }}<br>{% endif %}{% endfor %}</div>


                                            <div class="container-fluid containerHeader">
                                                <section class="row mb20pt">
                                                    <div class="col-md-12">
                                                        <div class="b-header__logo">
                                                            <a href="{{ site_url }}">
                                                                {% helper start.logo({type: 'user', settings: logo_settings}) %}
                                                            </a>
                                                        </div> 
                                                    </div>	
                                                </section>
                                                <section class="row mt13">
                                                    <nav class="navbar navbar-default upperRefs">
                                                        <div class="navbar-header floatLeft">
                                                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbarCollapsed">
                                                                <span class="sr-only"></span>
                                                                <span class="icon-bar"></span>
                                                                <span class="icon-bar"></span>
                                                                <span class="icon-bar"></span>
                                                            </button>
                                                        </div>
                                                        <div class="collapse navbar-collapse floatLeft" id="navbarCollapsed">
                                                            <ul class="nav navbar-nav navbar-left" id="leftNavbar">
                                                                <li>
                                                                    <a href="{{ site_url }}">{% helper lang.l('home', 'start') %}</a>
                                                                </li>
                                                                <li>
                                                                    <a href="{{ site_root }}content/view/about-us">{% helper lang.l('how_it_works', 'start') %}</a>
                                                                </li>
                                                                <li>
                                                                    <a href="{{ site_root }}users/search">{% helper lang.l('top_travelers', 'start') %}</a>
                                                                </li>
                                                                <li>
                                                                    <a href="{{ site_root }}media/index">{% helper lang.l('photos', 'start') %}</a>
                                                                </li>

                                                            </ul>
                                                        </div>
                                                        <div class="notCollapsed">
                                                            <div class="col-xs-12">
                                                                <ul class="nav navbar-nav navbar-right">
                                                                    <li>
                                                                        <a id="btn_log" class="btn_border" onclick="{% helper start.setAnalytics('index', 'btn_sign_in') %}"  
                                                                           href="{% helper seo.seolink('users', 'login_form') %}">
                                                                            {% helper lang.l('btn_login', 'start') %}</a>
                                                                    </li>
                                                                    <li>
                                                                        {% helper reglang = lang.l('join_now', 'start') %}
                                                                        {% helper users.usersRegistration({'is_link': 1, 'page': 1, 'lang': reglang}) %}
                                                                    </li>
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </nav>
                                                </section>
                                            </div>
                                            <div class="splitrow">
                                                <div class="splitrow__green th-set__main_bg">
                                                    <div class="splitrow__grcontent th-set__index_promo_text_main">
                                                        <h2>{% helper lang.l('find_travel_partner', 'start') %}</h2>
                                                        <p>{% helper lang.l('find_travel_partner_txt', 'start') %}</p>

                                                        {% helper reglang = lang.l('join_now', 'start') %}
                                                        {% helper users.usersRegistration({'is_link': 1, 'page': 1, 'lang': reglang}) %}
                                                    </div>
                                                </div>
                                                <div class="splitrow__chess">
                                                    <table class="chessrow th-set__index_promo_text_on_bg">
                                                        <tr>
                                                            <td>{% helper lang.l('clean_conversation', 'start') %}</td>
                                                            <td>
                                                                <img src="{{ site_root }}uploads/themes-fixed/bonatea/images/cells-2.jpg" alt="" class="img-responsive">
                                                                    <figcaption></figcaption>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <img src="{{ site_root }}uploads/themes-fixed/bonatea/images/cells-3.jpg" alt="" class="img-responsive">
                                                                    <figcaption></figcaption>
                                                            </td>
                                                            <td>{% helper lang.l('travel_companions', 'start') %}</td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </div>
                                            <main class="container-fluid">
                                                <div class="row addDeviders">
                                                    <section class="col-md-6 col-sm-12 addDevidersToCols1">
                                                        <h3 class="mapps__title">{% helper lang.l('mobile_app_title', 'start') %}</h3>
                                                        <div class="mapps__list">
                                                            {% helper mobile.mobileAppLinks({'viewtype': 'btnsBlack'}) %}
                                                            <a href="{{ site_root }}m/">{% helper lang.l('mobilev', 'start') %}</a>
                                                        </div>
                                                    </section>

                                                    <section class="col-md-6 col-sm-12 addDevidersToCols2">
                                                        <h3 class="mobileApps h3mobApps">{% helper lang.l('travel_partnership', 'start') %}</h3>
                                                        <p class="partnership">{% helper lang.l('travel_partnership_txt', 'start') %}</p>
                                                    </section>
                                                </div>
                                            </main>
                                            <footer class="th-set__index_footer_bg container-fluid">
                                                <div class="row rlPadds">
                                                    <section id="footer-list" class="col-sm-8 col-md-5 noPadds">
                                                        <ul class="footer-nav">
                                                            <li>
                                                                <a href="{{ site_url }}site_map">{% helper lang.l('sitemap', 'start') %}</a>
                                                            </li>
                                                            <li>
                                                                <a href="{{ site_root }}contact_us/index/">{% helper lang.l('contacts', 'start') %}</a>
                                                            </li>	
                                                            <li>
                                                                <a href="{{ site_url }}content/view/about-us">{% helper lang.l('about_us', 'start') %}</a>
                                                            </li>
                                                            <li>
                                                                <a href="{{ mobile_url }}" class="btn_tnl" data-bind="mobile">
                                                                    {% helper lang.l('mobile_version', 'mobile') %}
                                                                </a>
                                                            </li>	
                                                        </ul>
                                                    </section>
                                                    <section class="col-sm-4 col-md-2 text-center noPadds" id="languages">
                                                        <div class="footer__lang">{% helper users.users_lang_select({type: 'menu', template: 'dropup'}) %}</div>
                                                    </section>

                                                    <section id="copyright" class="col-sm-12 col-md-5 copyright noPadds">
                                                        {%if DEMO_MODE %}{{ demo_copyright }}{% else %}{% helper start.getCopyright('index_page') %}{% endif %}
                                                    </section>
                                                </div>
                                            </footer>
                                        </div>
                                        <!-- pjaxcontainer -->
                                        {% helper cookie_policy.cookie_policy_block() %}
                                        {% helper languages.lang_editor() %}
                                        {% helper seo_advanced.seo_traker('footer') %}
                                        {% helper start.intercom() %}
                                        {% helper start.amplitude() %}
                                        {% helper mobile.appTopBanner() %}
                                    </body>
                                    </html>
