                } else {
                    jQuery(this).addClass('active');
                }
                jQuery('#view_demos_desktop').trigger('click');
            });
            jQuery('#view_demos_desktop').bind('click', function (e) {
                e.preventDefault();
                e.stopPropagation();

                if (jQuery('.product-menu').is(':visible')) {
                    jQuery(this).removeClass('active');
                    jQuery('.sandwich-button').removeClass('active');
                    jQuery(this).find('.fa').removeClass('fa-caret-up').addClass('fa-caret-down');

                    jQuery('.product-menu').hide();
                    jQuery('#explore_menu').hide();
                } else {
                    jQuery(this).addClass('active');
                    if (!jQuery('.sandwich-button').hasClass('active')) {
                        jQuery('.sandwich-button').addClass('active');
                    }
                    jQuery(this).find('.fa').addClass('fa-caret-up').removeClass('fa-caret-down');

                    jQuery('.product-menu').css('display', 'flex');
                }
            });
            jQuery('#view_demos_mobile').bind('click', function (e) {
                e.preventDefault();
                e.stopPropagation();

                if (jQuery('.product-menu').is(':visible')) {
                    jQuery('.b-topnav').trigger('click');
                } else {
                    jQuery('#view-demo').trigger('click');
                }
            });
            jQuery('#btn-collapse').bind('click', function (e) {
                e.preventDefault();
                jQuery('#demo-panel').hide();
                $('#dashboard').css('top', '70px');
                $('.dashboard-visibility-btn').css('top', '87px');
                jQuery('#frame-wrapper').addClass('hide-panel');
                jQuery('#btn-collapse').addClass('hidden');
                jQuery('#btn-expand').removeClass('hidden');
                if (jQuery('.product-menu').is(':visible')) {
                    jQuery('.product-menu').hide();
                }
            });
            jQuery('#btn-expand').bind('click', function (e) {
                e.preventDefault();
                $('#dashboard').css('top', '167px');
                $('.dashboard-visibility-btn').css('top', '184px');
                jQuery('#demo-panel').show();
                jQuery('#frame-wrapper').removeClass('hide-panel');
                jQuery('#btn-collapse').removeClass('hidden');
                jQuery('#btn-expand').addClass('hidden');
                if (jQuery('.sandwich-button').hasClass('active')) {
                    jQuery('.product-menu').show();
                }
            });
            jQuery(function ($) {
                $('#demo_viewer_user_mode').bind('click', function (e) {
                    e.preventDefault();
                    localStorage.setItem('access_page', 'user');
                    $('#demo_viewer_user_mode').hide();
                    $('#demo_viewer_admin_mode').hide();
                    window.stop();
                    $('.demo-loader-switcher').show();
